<?xml version='1.0' encoding='utf-8'?>
<widget id="org.radarbase.cordova.app.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>radar-prmt-cordova-test</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <platform name="android">
        <preference name="GradlePluginKotlinEnabled" value="true" />
        <preference name="GradlePluginKotlinVersion" value="1.7.21" />
        <preference name="android-minSdkVersion" value="24"/>

        <preference name="AndroidXEnabled" value="true" />
    </platform>
</widget>
